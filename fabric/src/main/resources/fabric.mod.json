{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "sources": "${sources_url}",
    "issues": "${issues_url}"
  },
  "license": "${license}",
  "icon": "${mod_id}_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.illusivesoulworks.spectrelib.SpectreFabricMod"
    ],
    "client": [
      "com.illusivesoulworks.spectrelib.SpectreClientFabricMod"
    ],
    "preLaunch": [
      "com.illusivesoulworks.spectrelib.SpectrePreLaunchFabricMod"
    ],
    "modmenu": [
      "com.illusivesoulworks.spectrelib.integration.ModMenuPlugin"
    ]
  },
  "mixins": [
    "spectrelib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric-api": "*",
    "minecraft": "${minecraft_version_range_alt}",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
  